{
  "name": "typescript-starter",
  "version": "0.1.0",
  "description": "Typescript minimal starter project",
  "keywords": [
    "typescript",
    "starter"
  ],
  "repository": "https://github.com/jellydn/typescript-mini-starter",
  "license": "MIT",
  "author": "Huynh Duc Dung",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs",
    "types": "./dist/index.d.ts"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": "./dist/cli.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "yarn clean",
    "build": "pkgroll --env.NODE_ENV=production --minify --sourcemap",
    "build:dev": "yarn clean && pkgroll",
    "clean": "rimraf dist",
    "example": "npx tsx ./example/index.ts",
    "prepack": "clean-pkg-json",
    "test": "vitest --run",
    "test:dev": "vitest --ui",
    "test:tsd": "tsd",
    "typecheck": "tsc --noEmit",
    "watch": "pkgroll --watch"
  },
  "devDependencies": {
    "@skypack/package-check": "0.2.2",
    "@vitest/ui": "0.34.7",
    "clean-pkg-json": "1.2.0",
    "husky": "8.0.3",
    "nano-staged": "0.8.0",
    "pkgroll": "2.0.1",
    "rimraf": "5.0.5",
    "sort-package-json": "2.6.0",
    "tsd": "0.29.0",
    "tsx": "4.6.2",
    "typescript": "5.3.3",
    "vite": "5.0.8",
    "vitest": "0.34.6"
  },
  "packageManager": "yarn@1.22.21",
  "tsd": {
    "directory": "tests"
  }
}
